{
    "name": "Leakuidator DNR",
    "version": "1",

    "declarative_net_request": {
      "rule_resources": [{
        "id": "ruleset_1",
        "enabled": false,
        "path": "rules2.json"
      }]
    },

    "permissions": [
      "declarativeNetRequest",
      "declarativeNetRequestWithHostAccess",
      "declarativeNetRequestFeedback",
      "webRequest",
      "tabs",
      "activeTab"
    ],
    
    "host_permissions": [
      "<all_urls>"
    ],

    "background": {
      "service_worker": "background_migrated.js",
      "type": "module"
    },

    "manifest_version": 3
  }